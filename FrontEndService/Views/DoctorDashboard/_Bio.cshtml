@model FrontEndService.Models.WebDTO.BioDTO.BioDTO

<div class="tab active" id="doctor-bio">
    <h2>Doctor Bio</h2>
    <div class="doctor-info">
        <p><strong>Full Name:</strong> @Model.FullName</p>
        <p><strong>Specialization:</strong> @Model.Specialization</p>
        <p><strong>Contact Number:</strong> @Model.ContactNumber</p>
        <p><strong>Email:</strong> @Model.EmailAddress</p>
        <p><strong>Office Address:</strong> @Model.OfficeAddress</p>
    </div>
</div>
